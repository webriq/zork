{
    "name": "webriqcms/zork",
    "description": "Extensions for Zend Framework 2",
    "license": "CC-BY-SA-3.0",
    "keywords": [
        "zend",
        "framework",
        "zf2",
        "webriqcms"
    ],
    "homepage": "http://webriq.com/",
    "authors": [
        {
            "name": "WebriQ Dev Team",
            "email": "info@webriq.com",
            "homepage": "http://webriq.com/",
            "role": "Development Team"
        }
    ],
    "support": {
        "email": "info@webriq.com",
        "source": "https://github.com/webriq/zork"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-gd": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-fileinfo": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "zendframework/zendframework": "2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7"
    },
    "autoload": {
        "psr-0": {
            "Zork": "src/"
        }
    }
}
